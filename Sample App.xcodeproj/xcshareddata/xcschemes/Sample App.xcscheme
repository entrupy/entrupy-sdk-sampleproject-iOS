               scriptText = "# Path to the workspace directory&#10;WORKSPACE_DIR=$(dirname &quot;$WORKSPACE_PATH&quot;)&#10;&#10;# Path to your project&apos;s .pbxproj&#10;PBXPROJ_PATH_SDK=&quot;$WORKSPACE_DIR/EntrupySDK/EntrupySDK.xcodeproj/project.pbxproj&quot;&#10;# Check if running on simulator&#10;if [ &quot;$PLATFORM_NAME&quot; == &quot;iphonesimulator&quot; ]; then&#10;    echo &quot;Running on Simulator&quot;&#10;    &#10;        # Check if the TARGET_ENTRUPY macro exists and IS_AUTOMATED_TEST doesn&apos;t&#10;    if grep -q &quot;TARGET_ENTRUPY=1&quot; &quot;$PBXPROJ_PATH_SDK&quot; &amp;&amp; ! grep -q &quot;IS_AUTOMATED_TEST=1&quot; &quot;$PBXPROJ_PATH_SDK&quot;; then&#10;        # Append IS_AUTOMATED_TEST=1 to the existing macro&#10;        sed -i &apos;&apos; &apos;s/TARGET_ENTRUPY=1/TARGET_ENTRUPY=1 IS_AUTOMATED_TEST=1/&apos; &quot;$PBXPROJ_PATH_SDK&quot;&#10;        echo &quot;Injected IS_AUTOMATED_TEST macro for Simulator&quot;&#10;    else&#10;        echo &quot;No injection of IS_AUTOMATED_TEST macro for Simulator&quot;&#10;    fi&#10;else&#10;    echo &quot;Running on Real Device&quot;&#10;    &#10;    # Check if IS_AUTOMATED_TEST exists and remove it&#10;    if grep -q &quot;IS_AUTOMATED_TEST=1&quot; &quot;$PBXPROJ_PATH_CLIENT&quot;; then&#10;        sed -i &apos;&apos; &apos;s/IS_AUTOMATED_TEST=1//g&apos; &quot;$PBXPROJ_PATH_CLIENT&quot;&#10;        &#10;        if [[ $? -ne 0 ]]; then&#10;            echo &quot;Error modifying .pbxproj file&quot;&#10;            exit 1&#10;        fi&#10;        &#10;        echo &quot;Removed IS_AUTOMATED_TEST macro for Real Device&quot;&#10;    else&#10;        echo &quot;IS_AUTOMATED_TEST macro not found&quot;&#10;    fi&#10;    &#10;        # Check if IS_AUTOMATED_TEST exists and remove it&#10;    if grep -q &quot;IS_AUTOMATED_TEST=1&quot; &quot;$PBXPROJ_PATH_SDK&quot;; then&#10;        sed -i &apos;&apos; &apos;s/IS_AUTOMATED_TEST=1//g&apos; &quot;$PBXPROJ_PATH_SDK&quot;&#10;        &#10;        if [[ $? -n